{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "root": "{workspaceRoot}/packages/web-components-css",
  "sourceRoot": "{projectRoot}",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": ["{projectRoot}/dist"],
      "options": {
        "command": "npm run build",
        "cwd": "packages/web-components-css",
        "outputPath": "packages/web-components-css/dist"
      },
      "dependsOn": ["^build"]
    }
  }
}
