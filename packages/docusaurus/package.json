{
  "name": "@nl-rvo/docusaurus",
  "author": "Community for NL Design System",
  "version": "0.0.1",
  "description": "Documentation website for the ROOS Design System",
  "license": "EUPL-1.2",
  "keywords": [
    "rvo",
    "roos",
    "nl-design-system"
  ],
  "private": true,
  "repository": {
    "type": "git+ssh",
    "url": "git@github.com:nl-design-system/rvo.git"
  },
  "files": [
    "build/"
  ],
  "scripts": {
    "copy:docs": "rimraf docs/tmp && mkdirp docs/tmp && cpx '../../documentation/website/**/**' docs/tmp",
    "copy:components": "rimraf tmp && mkdirp tmp && cpx '../../components/**/*.md' tmp",
    "docusaurus": "docusaurus",
    "prestart": "npm-run-all --parallel copy:**",
    "start": "npm-run-all --parallel docs:**",
    "docs:start": "docusaurus start",
    "docs:watch": "cpx '../../documentation/website/**/*.md' docs/tmp --watch",
    "docs:watch-components": "cpx '../../components/**/*.md' tmp --watch",
    "prebuild": "npm-run-all --parallel copy:**",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc"
  },
  "devDependencies": {
    "@docusaurus/core": "2.2.0",
    "@docusaurus/preset-classic": "2.2.0",
    "@mdx-js/react": "1.6.22",
    "clsx": "1.2.1",
    "prism-react-renderer": "1.3.5",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "@docusaurus/module-type-aliases": "2.2.0",
    "@tsconfig/docusaurus": "1.0.5",
    "typescript": "4.7.4",
    "@nl-rvo/components": "workspace:*",
    "@nl-rvo/design-tokens": "workspace:*",
    "@nl-rvo/basiselementen-design-tokens": "workspace:*"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
