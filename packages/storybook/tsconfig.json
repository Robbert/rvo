{
  "compilerOptions": {
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "allowUnreachableCode": false,
    "declaration": false,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "target": "es2020",
    "jsx": "react-jsx",
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "resolveJsonModule": true,
    "types": ["node"],
    "allowJs": false,
    "baseUrl": ".",
    "paths": {
      "@nl-rvo/design-tokens/*": ["../../proprietary/design-tokens/*"],
      "@nl-rvo/assets/*": ["../../proprietary/assets/*"],
      "@docusaurus/components/*": ["../docusaurus/src/components/*"],
      "@storybook/addon-links": ["node_modules/@storybook/addon-links"],
      "@bootstrap-icons/*": ["node_modules/bootstrap-icons/*"]
    }
  },
  "include": ["config"],
  "exclude": ["node_modules"]
}
