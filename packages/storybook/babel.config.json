{
  "sourceType": "unambiguous",
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "chrome": 100
        }
      }
    ],
    [
      "@babel/preset-typescript",
      {
        "onlyRemoveTypeImports": true,
        "allExtensions": true,
        "isTSX": true
      }
    ],
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ]
  ],
  "plugins": ["macros", "@babel/plugin-syntax-flow"]
}
