{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "root": "{workspaceRoot}/proprietary/basiselementen-design-tokens",
  "sourceRoot": "{projectRoot}",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": ["{projectRoot}/dist"],
      "options": {
        "command": "npm run build",
        "cwd": "proprietary/basiselementen-design-tokens",
        "outputPath": "proprietary/basiselementen-design-tokens/dist"
      },
      "dependsOn": ["^build"]
    },
    "watch": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "npm run watch",
        "cwd": "proprietary/basiselementen-design-tokens"
      }
    }
  }
}
